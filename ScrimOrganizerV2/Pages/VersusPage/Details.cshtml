@page
@model ScrimOrganizerV2.Pages.VersusPage.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>VersusTeam</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VersusTeam.)
        </dt>
        <dd>
            @foreach (var summoner in Model.ShowStatsSummoner)
            {
                <div class="SummonerNames">
                    @Html.DisplayName(summoner.summonerName)
                    <br />
                    <div class="SummonerStats">
                        @Html.DisplayName(summoner.tier)
                        @Html.DisplayName(summoner.rank)
                    </div>
                </div>
            }

        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VersusTeam.Team2ID)
        </dt>
        @foreach (var summoner in Model.ShowStatsSummoner2)
        {
            <div class="SummonerNames">
                @Html.DisplayName(summoner.summonerName)
                <br />
                <div class="SummonerStats">
                    @Html.DisplayName(summoner.tier)
                    @Html.DisplayName(summoner.rank)
                </div>
            </div>
        }
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.VersusTeam.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
